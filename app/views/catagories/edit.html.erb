<h1> Edit catagory </h1>
<% if @catagory.errors.any? %>
<h3> Error </h3>
  <ul>
  <% @catagory.errors.full_messages.each do |msg| %>
  <li><%= msg %></li>
  <% end %>
  </ul>
<% end %>
<%= form_with(model: @catagory, local: true) do |f| %>

  <p>
    <%= f.label :title %><br/>
    <%= f.text_field :title %>
  </p>

  <p>
    <%= f.submit "Edit", :class=> "btn btn-danger"%>
  </p>

<% end %>